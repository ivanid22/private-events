<%= form_for @invitation do |f| %>
<%= f.label :attendee %>
<%= f.collection_select :attendee_id, all_other_users, :id, :name %>
<%= f.label :attended_event %>
<%= f.collection_select :attended_event_id, events_by( User.find(session[:user_id]) ), :id, :title %>

<%= f.submit :class => 'btn btn-primary pos' %>
<% end %>